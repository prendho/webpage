{{!< default}}
<div class="container-fluid">
  <div class="row">
    <div class="col-xs-offset-2 col-xs-8 col-sm-10 col-sm-offset-1 col-md-offset-2 col-md-8">
      {{#get "posts" filter="slug:pagina-no-encontrada" as |item|}}
        {{#item}}
          <img src="{{image}}">
          <p>{{content}}</p>
          <a href="/" class="btn btn-lg btn-primary intro-cta-btn">inicio</a>
        {{/item}}
      {{/get}}
    </div>
  </div>
</div>
