<nav aria-label="Page navigation">
  <ul class="pagination" id="pagination-list">
    {{#if prev}}
      <li><a href="{{page_url prev}}" aria-label="Previous"><span class="glyphicon glyphicon-menu-left" aria-hidden="true"></span></a></li>
    {{/if}}
    {{#if next}}
      <li><a href="{{page_url next}}" aria-label="Next"><span class="glyphicon glyphicon-menu-right" aria-hidden="true"></span></a></li>
    {{/if}}
  </ul>
  <div class="hidden">
    <div id="page-total">{{pages}}</div>
    <div id="page-current">{{page}}</div>
    <div id="total-elements">{{total}}</div>
  </div>
</nav>
