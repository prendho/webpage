{{!< default}}
<section>
  <div class="consultingSection">
    <div class="container">
      <div class="row align-center">
        <div class="col-xs-8 col-sm-10 col-md-8">
          {{> "common-gallery-post" item=post}}
          <a href="#" class="btn btn-lg btn-primary intro-cta-btn">conoce más</a>
        </div>
      </div>
    </div>
  </div>

  <div class="processSection">
    <div class="container">
      <div class="row align-center">
        <div class="processSection-content">
          <div class="align-center">
            <div class="col-xs-12 col-sm-10 col-md-8">
              {{#get "posts" filter="slug:asesoria-procesos-incubacion" as |item|}}
                {{> "common-gallery-post" item=item}}
              {{/get}}
            </div>
          </div>
          <div class="row processSection-content-features">
            {{#get "posts" filter="tag:asesoria-procesos-incubacion" order="published_at asc"}}
              {{#foreach posts}}
                <div class="step align-middle align-center">
                  <img class="step-image" src="{{image}}">
                  <p class="step-title">{{title}}</p>
                  {{^if @last}}
                    <img class="step-arrow" src="{{asset "static/flechas_proces.png"}}">
                  {{/if}}
                </div>
              {{/foreach}}
            {{/get}}
          </div>
          <div class="button">
            <a href="#" class="btn btn-lg btn-primary inverted">Infórmate</a>
          </div>
        </div>
      </div>
    </div>
    <svg id="curveDownColor" xmlns="http://www.w3.org/2000/svg" version="1.1" width="100%" height="100" viewBox="0 0 100 100" preserveAspectRatio="none">
      <path d="M0 0 C 50 100 80 100 100 0 Z"/>
    </svg>
  </div>

  <div class="carouselSection">
    <div class="container">
      <div class="row align-center">
        <div class="post-gallery col-xs-8 col-sm-10 col-md-11 col-lg-12">
          {{#get "posts" filter="slug:laboratorios-carrusel" as |item|}}
            {{> "common-gallery-post" item=item}}
          {{/get}}
          <div class="responsive-carousel">
            {{#get "posts" filter="tag:laboratorios-carrusel" as |items|}}
              {{> "carousel-items" items=items}}
            {{/get}}
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
